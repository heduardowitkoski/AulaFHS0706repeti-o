#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b;

    printf("Insira dois numeros: \n");
    scanf("%d%d",&a,&b);

    if(a<b)
        printf("%d e menor que %d",a,b);
    if(b<a)
        printf("%d e menor que %d",b,a);
    if(a==b)
        printf("%d e %d sao iguais",a,b);

    return 0;
}
