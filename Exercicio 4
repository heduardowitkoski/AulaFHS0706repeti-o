#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c;

    printf("Insira tres numeros: \n");
    scanf("%d%d%d",&a,&b,&c);

    if(a==b || a==c || b==c)
        printf("Insira numeros diferentes\n");

    else
        {
        if(a>b && a>c)
            printf("\n%d e o maior",a);
        if(b>a && b>c)
            printf("\n%d e o maior",b);
        if(c>a && c>b)
            printf("\n%d e o maior",c);
        }

    return 0;
}
