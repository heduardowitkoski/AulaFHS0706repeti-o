#include <stdio.h>
#include <stdlib.h>

int main()
{
    float nota1,nota2,media;

    printf("Insira suas duas notas: \n");
    scanf("%f%f",&nota1,&nota2);

    media=(nota1+nota2)/2;

    printf("Sua media e: %.1f",media);

    if(media>=0 && media<3)
        printf("\nReprovado");
    if(media>=3 && media<7)
        printf("\nExame");
    if(media>=7 && media<=10)
        printf("\nAprovado");


    return 0;
}
